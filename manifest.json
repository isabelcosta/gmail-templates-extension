{
  "name": "Isabel Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": [
        "static/inboxsdk.js",
        "static/react.development.js",
        "static/react-dom.development.js",
        "static/babel.min.js",
        "content.js", 
        "TemplatesList.js"
      ],
      "run_at": "document_end"
    }
  ],
  "content_security_policy": "script-src 'self' https://unpkg.com https://xyz-amplemarket-challenge.herokuapp.com ; object-src 'self'",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
